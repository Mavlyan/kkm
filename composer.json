{
    "name": "mygento/kkm",
    "type": "magento-module",
    "license": "proprietary",
    "homepage": "https://github.com/mygento/kkm",
    "description": "Модуль интеграции фискальных кассовых аппаратов для Magento в соответствии с 54-ФЗ",
    "keywords": [
        "magento",
        "payment"
    ],
    "authors": [
        {
            "name": "Mygento",
            "email": "connect@mygento.net",
            "homepage": "https://www.mygento.ru/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-bcmath": "*",
        "magento-hackathon/magento-composer-installer": "3.*",
        "mygento/base": "1.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
