        "mygento/coding-standard": "1.*",
        "sebastian/phpdcd": "*",
        "sebastian/phpcpd": "*",
        "jakub-onderka/php-parallel-lint": "0.*",
        "jakub-onderka/php-console-highlighter": "0.*",
        "phpmd/phpmd": "@stable"
    },
    "extra": {
        "magento-root-dir": ".",
        "with-bootstrap-patch": false
